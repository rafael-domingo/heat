      time: '12:00PM',\n                id: 800,\n                icon: '01d',\n                description: 'clear'\n            },\n            {\n                time: '1:00PM',\n                id: 804,\n                icon: '04d',\n                description: 'overcast clouds'\n            },\n            {\n                time: '2:00PM',\n                id: 800,\n                icon: '01d',\n                description: 'clear'\n            },\n            {\n                time: '3:00PM',\n                id: 804,\n                icon: '04d',\n                description: 'overcast clouds'\n            }\n        ],\n        css: 'linear-gradient(120deg, #f093fb 0%, #f5576c 100%)'\n    },\n    {\n        name: 'Hydration',\n        value: 'Drink 4 cups / hour',\n        hourly: [],\n        css: 'linear-gradient(-225deg, #E3FDF5 0%, #FFE6FA 100%)'\n    },\n    {\n        name: 'Heat Stress Symptoms',\n        value: 'Click to open',\n        hourly: [],\n        css: 'linear-gradient(to top, #d299c2 0%, #fef9d7 100%)'\n    },\n    {\n        name: 'First Aid',\n        value: 'Click to open',\n        hourly: [],\n        css: 'linear-gradient(to top, #ebc0fd 0%, #d9ded8 100%)'\n    }\n]","/Users/rafaeldomingo/Documents/GitHub/heat/src/components/grid/Cell.js",["106","107","108","109","110","111","112"],"import React from 'react'\nimport Slug from './Slug'\nimport Fade from './Fade'\nimport {symptoms} from '../../util/Symptoms'\nimport Symptoms from '../content/symptoms'\nimport Default from '../content/default'\nimport Hydration from '../content/hydration'\nimport Conditions from '../content/conditions'\nimport FirstAid from '../content/firstAid'\nimport Precautions from '../content/precautions'\n\nimport { GraphContainer } from '../graph/GraphContainer';\n\nimport './styles.css'\n\n\nexport default class Cell extends React.Component {\n    render() {\n        const { toggle, name, value, css, active, hourly } = this.props\n        if (name === 'Conditions') {\n            var result = <Conditions hourly={hourly} />\n        } else if (hourly.length > 0) {\n            var result = <Default hourly={hourly} />\n        } else if (name === 'Hydration') {\n            var result = <Hydration />\n        } else if (name === 'First Aid') {\n            var result = <FirstAid symptoms={symptoms} />\n        } else if (name === 'Heat Stress Symptoms') {\n            var result = <Symptoms symptoms={symptoms} />\n        } else if (name === 'Risk Level') {\n            var result = <Precautions risk={value}/>\n        } else {\n            var result = <div></div>\n        }\n        return (\n            <div\n            className=\"cell\"\n            style={{ backgroundImage: css, cursor: !active ? 'pointer' : 'auto' }}\n            onClick={!active ? toggle : undefined}>\n                <Fade show={active} delay={active ? 500 : 0}>\n                    <div className=\"details\">\n                        <Slug delay={600}>\n                        <h2>{name}</h2>\n                        {result}\n                        </Slug>\n                    </div>\n                </Fade>\n                <Fade\n                show={!active}\n                from={{ opacity: 0}}\n                enter={{ opacity: 1}}\n                leave={{ opacity: 0}}\n                // from={{ opacity: 0, transform: 'translate3d(0,140px,0)'}}\n                // enter={{ opacity: 1, transform: 'translate3d(0,0px,0)'}}\n                // leave={{ opacity: 0, transform: 'translate3d(0,-50px,0)'}}\n                delay={active ? 0 : 1000}>\n                    <div className=\"default\">\n                        <div style={{ zIndex: 1}}>\n                            <h1>{value}</h1>\n                            <p>{name}</p>\n                        </div>\n                    </div>\n\n                </Fade>\n            </div>\n        )\n    }\n}","/Users/rafaeldomingo/Documents/GitHub/heat/src/components/grid/Slug.js",["113"],"import React from 'react'\nimport { Transition, Trail, animated } from 'react-spring/renderprops'\n\nexport default class Slug extends React.PureComponent {\n    render() {\n        const { \n            children, \n            from = { opacity: 0 },\n            to = { opacity: 1},\n            // from = { opacity: 0, transform: `translate3d(0,40px,0)` },\n            // to = { opacity: 1, transform: `translate3d(0, 0px, 0)`},\n            ...rest\n        } = this.props\n        const result = React.Children.map(children, child => styles => {\n            const Component = animated[child.type] || animated(child.type)\n            const props = {\n                ...child.props,\n                style: {\n                    willChange: 'opacity, transform',\n                    ...child.props.style,\n                    ...styles\n                }\n            }\n            return <Component {...props} />\n        }) \n        return (\n            <Trail\n                native\n                {...rest}\n                items={result}\n                keys={result.map((_, i) => i)}\n                from={from}\n                to={to}\n                children={child => child}\n            />\n        )\n    }\n}","/Users/rafaeldomingo/Documents/GitHub/heat/src/components/grid/Fade.js",["114"],"import React from 'react'\nimport { Transition, Trail, animated } from \"react-spring/renderprops\";\n\nexport default class Fade extends React.PureComponent {\n    render() {\n        const {\n            children, \n            show, \n            from = { opacity: 0 },\n            enter = { opacity: 1 },\n            leave = { opacity: 0 },\n            ...rest\n        } = this.props\n\n        const { type, props } = children\n        const Component = animated[type] || animated(type)\n        const result = styles => {\n            const newProps = {\n                ...props,\n                style: {\n                    willChange: 'opacity, transform',\n                    ...props.style,\n                    ...styles\n                }\n            }\n            return <Component {...newProps} />\n        }\n        \n        return (\n            <Transition \n                native\n                items={show}\n                {...rest}\n                from={from}\n                enter={enter}\n                leave={leave}\n                children={show => show && result}\n            />\n        )\n    }\n}","/Users/rafaeldomingo/Documents/GitHub/heat/src/util/Symptoms.js",[],"/Users/rafaeldomingo/Documents/GitHub/heat/src/components/content/symptoms.js",[],"/Users/rafaeldomingo/Documents/GitHub/heat/src/components/content/default.js",[],"/Users/rafaeldomingo/Documents/GitHub/heat/src/components/content/hydration.js",[],"/Users/rafaeldomingo/Documents/GitHub/heat/src/components/content/conditions.js",["115","116","117"],"import React from 'react';\nimport './conditions.css';\n\nexport default class Conditions extends React.Component {\n    render() {\n        const conditions = this.props.hourly.map(hourly => <img src={`http://openweathermap.org/img/wn/${hourly.icon}@2x.png`} style={{ width: '100%', height: '100%'}}/>)\n        const firstCondition = conditions[0]\n        const remainingConditions = conditions.slice(1)\n        const times = this.props.hourly.map(hourly => <p style={{ width: '100%', height: '100%', lineHeight: '20px'}}>{hourly.time}</p>)\n        const firstTime = times[0]\n        const remainingTimes = times.slice(1)\n        const descriptions = this.props.hourly.map(hourly => <p style={{ width: '100%', height: '100%', lineHeight: '20px'}}>{hourly.description}</p>)\n        const firstDescription = descriptions[0]\n        const remainingDescriptions = descriptions.slice(1)\n\n        return (\n            <div className=\"conditions\">\n                <div className=\"conditions-current\">\n                    <div>\n                        {firstTime}\n                    </div>\n    